{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/ft-press-release",
  "version": "0.1.0",
  "title": "Ft Press Release",
  "category": "widgets",
  "icon": "superhero-alt",
  "description": "Example block scaffolded with Create Block tool.",
  "keywords": [
    "press"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "h3"
    },
    "presstext": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "instructions": {
      "type": "string",
      "source": "html",
      "selector": ".steps"
    },
    "align": {
      "type": "string",
      "default": "none"
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "card",
      "label": "Card"
    }
  ],
  "supports": {
    "html": false
  },
  "textdomain": "ft-press-release",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}